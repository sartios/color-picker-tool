{
  "name": "savramis-react-demo",
  "private": true,
  "version": "0.0.1",
  "description": "Savramis Sartios - React Color Picker Demo",
  "main": "",
  "scripts": {
    "postinstall": "webpack -p --config --env=dist --progress",
    "start": "node server.js --env=dist",
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
    "dist": "npm run copy & webpack --env=dist",
    "lint": "eslint ./src",
    "posttest": "npm run lint",
    "release": "webpack -p --env=dev && npm run copy",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist"
  },
  "repository": "",
  "keywords": [],
  "author": "Savramis Sartios",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babili-webpack-plugin": "0.0.7",
    "bower-webpack-plugin": "^0.1.9",
    "copyfiles": "^1.0.0",
    "css-loader": "^0.23.0",
    "eslint": "^3.5.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.2.2",
    "file-loader": "^0.9.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "node-sass": "^3.10.0",
    "react-hot-loader": "^1.2.9",
    "rimraf": "^2.4.3",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.2"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "color-harmony": "^0.3.0",
    "core-js": "^2.0.0",
    "hex-rgb": "^1.0.0",
    "hsl-to-hex": "^1.0.0",
    "normalize.css": "^4.0.0",
    "rc-color-picker": "^1.1.0",
    "react": "^15.0.0",
    "react-bootstrap-slider": "^1.0.6",
    "react-dom": "^15.0.0",
    "rgb-hex": "^2.0.0",
    "values.js": "^1.0.3"
  }
}
